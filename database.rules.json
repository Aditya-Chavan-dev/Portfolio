{
  "rules": {
    "metrics": {
      ".read": true,
      ".write": true
    },
    "system": {
      "owner": {
        ".read": "auth != null",
        ".write": "!data.exists() && newData.exists() && auth != null"
      }
    },
    "sources": {
      ".read": "auth != null",
      ".write": false
    }
  }
}