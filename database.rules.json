{
  "rules": {
    "test": {
      "message": {
        ".read": true,
        ".write": true
      }
    },
    "analytics": {
      ".read": "auth != null",
      ".write": true,
      "sessions": {
        "$sessionId": {
          ".write": true
        }
      }
    },
    "sources": {
      ".read": true,
      ".write": true
    },
    ".read": false,
    ".write": false
  }
}